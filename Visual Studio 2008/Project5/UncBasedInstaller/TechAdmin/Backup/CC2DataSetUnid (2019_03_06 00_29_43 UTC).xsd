            <MainSource>
              <DbSource ConnectionRef="CCIConnectionString (Settings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>;with cteUnid as
(SELECT unid ,Value 

FROM dbo.XMLParmsDtl
where keyname = 'CustomerName' and Value = @p1
)
,cteUserType as 
(SELECT distinct  value, unid 

FROM dbo.XMLParmsDtl
where keyname = 'UserType'  
)
,ctePath as
(Select  unid ,
coalesce(

 dbo.XmlShred(logmsg,'targetdir') , 'Unknown') as 'Path' from trans.CustomerLog where status = 99999
) 
Select  distinct cteUnid.unid as 'Customer ID'
,cteUnid.Value  as 'Customer Name'
,u.Value as 'UserName' 
,cteUserType.Value as 'UserType'
,coalesce(sch.Value,'NA') as 'School Number'
,coalesce(dept.Value,'NA') as 'Department'
,coalesce(pt.Path ,'Unknown') as 'instPath'
FROM dbo.XMLParmsDtl u 
inner join cteUnid on cteUnid.unid = u.unid
inner join cteUserType on cteUserType.unid = u.unid 
left join dbo.XMLParmsDtl sch
            on u.unid = sch.unid
            and sch.keyname = 'SchoolNum'
left join dbo.XMLParmsDtl dept
            on u.unid = dept.unid
            and dept.keyname = 'PrimaryDept'    
 left join ctePath pt
           on cteUnid.unid       = pt.unid   
where u.keyname = 'UserName' and u.unid = cteUnid.unid 
</CommandText>
